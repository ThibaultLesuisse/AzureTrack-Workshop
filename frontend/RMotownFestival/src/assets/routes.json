{
    "routes": [
      {
        "route": "/pictures",
        "serve": "/index.html/pictures",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/upload",
        "serve": "/index.html/upload",
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "allowedRoles": ["anonymous"],
        "statusCode": 200
      }],
    "platformErrorOverrides": [
      {
        "errorType": "NotFound",
        "serve": "/custom-404.html"
      },
      {
        "errorType": "Unauthenticated",
        "statusCode": "302",
        "serve": "/login"
      }
    ],
    "defaultHeaders": {
      "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "mimeTypes": {
        "custom": "text/html"
    }
  }